{
  "name": "parcel-plugin-modernizr",
  "version": "1.1.3",
  "main": "index.js",
  "license": "MIT",
  "repository": "https://github.com/hirasso/parcel-plugin-modernizr.git",
  "scripts": {
    "test": "mocha --timeout 50000"
  },
  "modernizr": {
    "classPrefix": "",
    "options": [
      "setClasses",
      "addTest",
      "html5printshiv",
      "load",
      "testProp",
      "fnBind"
    ],
    "feature-detects": [
      "css/pointerevents",
      "touchevents",
      "history"
    ]
  },
  "dependencies": {
    "modernizr": "^3.6.0",
    "fs-extra": "^7.0.0"
  },
  "devDependencies": {
    "mocha": "^5.2.0",
    "parcel-assert-bundle-tree": "^1.0.0",
    "parcel-bundler": "^1.9.7"
  },
  "peerDependencies": {
    "parcel-bundler": "^1.9.7"
  }
}
